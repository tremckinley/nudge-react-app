{
  "manifest_version": 3,
  "name": "Nudge, the Study Buddy",
  "version": "1.0",
  "description": "A study buddy extension to help you stay focused.",
  
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "./index.html"
  },
  "background": {
    "service_worker": "./background.js"
  }
}
