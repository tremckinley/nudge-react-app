{
  "manifest_version": 3,
  "name": "Nudge, the Study Buddy",
  "version": "1.0",
  "description": "A study buddy extension to help you stay focused.",
  "icons": {
    "16": "./icons.icon16.png",
    "48": "./icons/icon48.png",
    "128": "./icons/icon128.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "./index.html"
  },
  "background": {
    "service_worker": "./background.js"
  }
}
